{
    "name": "pomotimer",
    "version": "1.0.0",
    "description": "Internet Bowser's Pomodoro Timer",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --platform=darwin --arch=x64 --icon=pomo-mushroom.icns",
        "installer-mac": "electron-installer-dmg pomotimer-darwin-x64/pomotimer.app pomo-installer",
        "package-win": "electron-packager . --platform=win32 --icon=assets/icons/win/icon.ico",
        "package-linux": "electron-packager . --platform=linux --icon=assets/icons/png/icon.png"
    },
